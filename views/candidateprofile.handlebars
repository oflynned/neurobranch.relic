<div id="page-content-wrapper dashboard">
    <div class="container-fluid content-main">
        <div class="row">
            <div class="col-lg-12">
                <!-- title -->
                <div class="page-header">
                    <h3>{{candidate._id}}</h3>
                </div>
            </div>
        </div>

        <!-- container for holding trial data -->
        <div id="dashboard-content-holder">
            <div class="col-md-6 text-left">
                <div><strong>Contact Email:</strong> {{candidate.email}}</div>
                <button class="btn btn-primary">
                    <a href="mailto:{{candidate.email}}" style="color: white">Contact</a>
                </button>
            </div>

        </div>
    </div>
    {{#if eligibility}}
        <div id="dashboard-content-holder">
            <h3>Eligibility Results</h3>
            {{#each eligibility}}

            {{/each}}
        </div>
    {{/if}}
</div>